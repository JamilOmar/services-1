{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "target": "es6",
      "sourceMap": true,
      "noImplicitAny": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noImplicitThis": false,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "pretty": true,
      "lib": ["ES2015"]
    },
    "compileOnSave": true,
    "exclude": [
      "node_modules",
      "dist"
    ]
  }